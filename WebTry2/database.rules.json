{
  "rules": {
    ".read": true,
    ".write": true,
    "Workday": {
      "$namePer": {
        ".indexOn": ".key",
        "$namePer": {
          ".indexOn": "date"
        }
      }
    },
    "Pay Period Histories": {
      "$name": {
        ".indexOn": ".key"
      }
    },
    "Locations": {
      "$name": {
        ".indexOn": ".key"
      }
    },
    "Jobs": {
      "$nameLoc": {
        ".indexOn": ".key",
        "$ind": {
          ".indexOn": [
            "start",
            "end"
          ]
        }
      }
    },
    "Clients": {
      ".indexOn": ".key"
    },
    "Schedules": {
      ".indexOn": ".key",
      "$name": {
        ".indexOn": ".key"
      }
    }
  }
}
